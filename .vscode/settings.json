{
    "search.exclude": {
        "**/package-lock.json": true,
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.{min.js,min.js.map}": true
    },
    "files.exclude": {
        "Images": false,
        "**/.git": true,
        "**/.svn": true,
        "**/.DS_Store": true,
        "apple-*": true,
        "favicon*": true,
        "mstile*": true,
        "**/*.xlsx": true,
        "**/*.sql": true
    }
}